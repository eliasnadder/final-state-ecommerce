{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/build/$1" },
        { "src": "/storage/(.*)", "dest": "/storage/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://fainalystate.vercel.app",
        "APP_KEY": "base64:jLYXYVkMysogZTN67BINVchANWZtePJEpt/T8iSqARE=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    },
    "build": {
        "env": {
            "APP_ENV": "production"
        }
    },
    "buildCommand": "composer install --optimize-autoloader --no-dev && npm install && npm run build && php artisan migrate --force --no-interaction && php artisan config:cache && php artisan route:cache && php artisan view:cache",
    "outputDirectory": "public"
}
